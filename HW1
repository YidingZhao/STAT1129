color=["black","white","blue"]
print(color)

nums=list(range(30, 61, 3))
print(nums)
nums_tuple=tuple(range(30, 61, 3))
print(nums_tuple)

list=[]
for i in range(0,6):
    list.append(i)
print(list)
list.remove(2)
print(list)
list.insert(2,2.0)
print(list)
print(len(list))
print(max(list))
print(min(list))

m=list(range(11))
print(m)
n=sum(m)
print(n)
